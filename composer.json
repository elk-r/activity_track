{
    "name": "elf-r/activity_track",
    "description": "Track activity for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "authors": [
        {
            "name": "vschoholiev",
            "email": "vshoholev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ElfR\\ActivityTrack\\": "src"
        }
    },
    "require": {
        "php": ">=7.0",
        "illuminate/container": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~7.0.0|8.*",
        "illuminate/contracts": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~7.0.0|8.*",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|~7.0.0|8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|6.2|^7.0"
    }
}
